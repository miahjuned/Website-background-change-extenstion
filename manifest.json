{
    "name": "changecolor",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
     "icons":{
        "16": "icons/logo_16x16.png",
        "48": "icons/logo_48x48.png",
        "128": "icons/logo_128x128.png"
    },
    "browser_action": {
      "default_title": "Change Color",
      "default_popup":"popup.html"
    }
  }